{
  "name": "@ludw1gj/binary-fractal-tree",
  "version": "0.5.8",
  "description": "Draws a binary fractal tree using a HTML5 canvas and a 2D context.",
  "type": "module",
  "main": "./dist/binary-fractal-tree.umd.cjs",
  "module": "./dist/binary-fractal-tree.js",
  "exports": {
    ".": {
      "import": "./dist/binary-fractal-tree.js",
      "require": "./dist/binary-fractal-tree.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "ludw1gj",
  "license": "ISC",
  "keywords": [
    "HTMLCanvasElement",
    "DOM",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludw1gj/binary-fractal-tree.git"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.2"
  }
}
